#include<iostream>
#include<conio.h>
#include<string>
using namespace std;
void trimstring(string trim)
{
if (size(trim) > 10)
{
cout << trim.substr(0, 10);
}
else
{
cout << trim;
}
}
int main()
{
string text;
cout << "Enter A String" << endl;
getline(cin, text);
trimstring(text);
_getch();
return 0;
}
